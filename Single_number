/*This got accepted in the leetcode with the following:
Runtime: 12 ms, faster than 96.29% of C++ online submissions for Single Number.
Memory Usage: 9.8 MB, less than 74.07% of C++ online submissions for Single Number.
*/

class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int x=0;
        for(int i=0;i<nums.size();i++)
            x=x^nums[i];
        return x;
    }
};
