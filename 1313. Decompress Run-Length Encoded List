#approach 1
class Solution {
public:
    vector<int> decompressRLElist(vector<int>& nums) {
        vector<int> res;
        for(int i=0;i<nums.size();i+=2)
        {
            for(int j=1;j<=nums[i];j++)
                res.push_back(nums[i+1]);
        }
        return res;
    }
};


#approach 2
class Solution {
public:
    vector<int> decompressRLElist(vector<int>& nums) {
        vector<int> res;
        for(int i=0;i<nums.size();i+=2)
            res.insert(res.end(),nums[i],nums[i+1]);
        return res;
    }
};
